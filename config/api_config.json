{
  "api": {
    "host": "0.0.0.0",
    "port": 8000,
    "reload": true,
    "workers": 1
  },
  "cors": {
    "allow_origins": [
      "*"
    ],
    "allow_methods": [
      "GET",
      "POST"
    ],
    "allow_headers": [
      "*"
    ]
  },
  "models": {
    "default_version": "latest",
    "cache_models": true,
    "prediction_timeout": 30
  }
}